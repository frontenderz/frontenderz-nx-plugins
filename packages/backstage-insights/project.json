{
  "name": "backstage-insights",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/backstage-insights/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/backstage-insights",
        "main": "packages/backstage-insights/src/index.ts",
        "tsConfig": "packages/backstage-insights/tsconfig.lib.json",
        "assets": [
          "packages/backstage-insights/*.md",
          "packages/backstage-insights/executors.json",
          "packages/backstage-insights/generators.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/backstage-insights/jest.config.js"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "generate": {
      "executor": "./src/executors/generate:default",
      "options": {}
    }
  }
}

